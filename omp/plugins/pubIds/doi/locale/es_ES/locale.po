msgid "plugins.pubIds.doi.displayName"
msgstr "Identificador de objeto digital (DOI)"

msgid "plugins.pubIds.doi.readerDisplayName"
msgstr "DOI:"

msgid "plugins.pubIds.doi.manager.settings.description"
msgstr "Configure el complemento DOI para poder gestionar y usar los elementos DOI en OMP:"

msgid "plugins.pubIds.doi.manager.settings.doiObjectsRequired"
msgstr "Seleccione los elementos DOI que desea asignar."

msgid "plugins.pubIds.doi.manager.settings.explainDois"
msgstr "Seleccione los objetos publicados a los que desea asignar el Identificador de objetos digitales (DOI):"

msgid "plugins.pubIds.doi.manager.settings.doiPrefix"
msgstr "Prefijo DOI"

msgid "plugins.pubIds.doi.manager.settings.doiPrefix.description"
msgstr "El prefijo DOI se asigna por organizaciones de registro (p.ej. <a href=\"http://www.crossref.org\" target=\"_new\">CrossRef</a>) y se encuentra en el formato 10.xxxx (p. ej. 10.1234):"

msgid "plugins.pubIds.doi.manager.settings.doiPrefixPattern"
msgstr "El prefijo del DOI es obligatorio y debe respetar la forma 10.xxxx."

msgid "plugins.pubIds.doi.manager.settings.doiSuffix"
msgstr "Sufijo del DOI"

msgid "plugins.pubIds.doi.manager.settings.doiSuffix.description"
msgstr "El sufijo del DOI puede tener cualquier forma, pero debe ser único para cada elemento publicado con un mismo prefijo."

msgid "plugins.pubIds.doi.manager.settings.doiSuffixPattern.representations"
msgstr "para formatos de publicación"

msgid "plugins.pubIds.doi.manager.settings.doiSuffixPattern.example"
msgstr "Por ejemplo, seleccionar%ppub%f puede crear un DOI como 10.1234/pressESPpub100"

msgid "plugins.pubIds.doi.manager.settings.doiSuffixDefault"
msgstr "Usar modelos predeterminados."

msgid "plugins.pubIds.doi.manager.settings.doiRepresentationSuffixPatternRequired"
msgstr "Introduzca el modelo de sufijo del DOI para los formatos de publicación."

msgid "plugins.pubIds.doi.manager.settings.doiReassign"
msgstr "Asignar los DOI de nuevo"

msgid "plugins.pubIds.doi.manager.settings.doiReassign.confirm"
msgstr "¿Está seguro/a de que desea eliminar todos los DOI existentes?"

msgid "plugins.pubIds.doi.editor.doi"
msgstr "Identificador de objeto digital (DOI)"

msgid "plugins.pubIds.doi.editor.doiObjectTypeRepresentation"
msgstr "formato de publicación"

msgid "plugins.pubIds.doi.editor.doiSuffixCustomIdentifierNotUnique"
msgstr "El sufijo del DOI suministrado ya se encuentra en uso en otro elemento publicado. Introduzca un único sufjo del DOI para cada elemento."
