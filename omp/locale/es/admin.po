msgid "admin.settings.info.success"
msgstr "Información del portal actualizada correctamente."

msgid "admin.hostedContexts"
msgstr "Editoriales alojadas"

msgid "admin.settings.appearance.success"
msgstr "Apariencia del portal configurada correctamente."

msgid "admin.settings.config.success"
msgstr "La configuración del portal se ha actualizado correctamente."

msgid "admin.settings.redirect"
msgstr "Redirigir Editorial"

