msgid "admin.contexts.form.titleRequired"
msgstr "A title is required."

msgid "admin.contexts.form.pathRequired"
msgstr "A path is required."

msgid "admin.contexts.form.pathAlphaNumeric"
msgstr "The path can only include letters, numbers and the characters _ and -. It must begin and end with a letter or number."

msgid "admin.contexts.form.pathExists"
msgstr "The path you provided is already in use by another server."

msgid "admin.contexts.form.create.success"
msgstr "{$name} was created successfully."

msgid "admin.contexts.form.edit.success"
msgstr "{$name} was edited successfully."

msgid "admin.journals.enableJournalInstructions"
msgstr "Enable this server to appear publicly on the site"

msgid "admin.contexts.contextDescription"
msgstr "Server description"

msgid "admin.journal.pathImportInstructions"
msgstr "Existing server path or path to create (e.g., "ojs")."

msgid "admin.journals.transcode"
msgstr "Transcode preprint metadata from ISO8859-1"

msgid "admin.journals.redirect"
msgstr "Generate code to map PPS 1 URLs to PPS 2 URLs"

