msgid "admin.hostedContexts"
msgstr "Hosted Servers"

msgid "admin.settings.appearance.success"
msgstr "The site appearance settings have been successfully updated."

msgid "admin.settings.config.success"
msgstr "The site configuration settings have been successfully updated."

msgid "admin.settings.info.success"
msgstr "The site information has been successfully updated."

msgid "admin.settings.redirect"
msgstr "Server redirect"

msgid "admin.settings.redirectInstructions"
msgstr "Requests to the main site will be redirected to this server. This may be useful if the site is hosting only a single server, for example."

msgid "admin.languages.confirmDisable"
msgstr "Are you sure you want to disable this locale? This may affect any hosted servers currently using the locale."

msgid "admin.languages.downloadUnavailable"
msgstr ""
"<p>Downloading language packs from the Public Knowledge Project web server is not currently available because:</p>"
"\t<ul>"
"\t\t<li>Your server does not have or allow execution of the GNU \"tar\" utility.</li>"
"\t\t<li>PPS is unable to modify the locale registry file, typically \"registry/locales.xml\".</li>"
"\t</ul>"
"<p>Language packs can be manually downloaded from the <a href=\"http://pkp.sfu.ca\">PKP web site</a>.</p>"

msgid "admin.locale.maybeIncomplete"
msgstr "Marked locales may be incomplete."

msgid "admin.languages.confirmUninstall"
msgstr "Are you sure you want to uninstall this locale? This may affect any hosted servers currently using the locale."

msgid "admin.languages.installNewLocalesInstructions"
msgstr "Select any additional locales to install support for in this system. Locales must be installed before they can be used by hosted servers. See the PPS documentation for information on adding support for new languages."

msgid "admin.languages.localeInstalled"
msgstr "The "{$locale}" locale has been installed."

msgid "admin.languages.download"
msgstr "Download Locale"

msgid "admin.languages.download.cannotOpen"
msgstr "Cannot open language descriptor from PKP web site."

msgid "admin.languages.download.cannotModifyRegistry"
msgstr "Cannot add the new locale to the locale registry file, typically "registry/locales.xml"."

msgid "admin.systemConfiguration"
msgstr "PPS Configuration"

msgid "admin.contexts.create"
msgstr "Create Server"

msgid "admin.contexts.form.pathAlphaNumeric"
msgstr "The path can only include letters, numbers and the characters _ and -. It must begin and end with a letter or number."

msgid "admin.contexts.form.pathExists"
msgstr "The path you provided is already in use by another server."

msgid "admin.contexts.form.create.success"
msgstr "{$name} was created successfully."

msgid "admin.contexts.form.edit.success"
msgstr "{$name} was edited successfully."

msgid "admin.contexts.contextDescription"
msgstr "Server description"

