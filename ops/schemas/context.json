{
	"title": "Journal",
	"description": "A journal.",
	"properties": {
		"abbreviation": {
			"type": "string",
			"multilingual": true,
			"apiSummary": true,
			"validation": [
				"nullable"
			]
		},
		"copyrightYearBasis": {
			"type": "string",
			"default": "issue",
			"validation": [
				"in:issue,article"
			]
		},
		"enableOai": {
			"type": "boolean",
			"default": 1,
			"validation": [
				"nullable"
			]
		},
		"journalThumbnail": {
			"type": "object",
			"multilingual": true,
			"apiSummary": true,
			"validation": [
				"nullable"
			],
			"properties": {
				"temporaryFileId": {
					"type": "integer",
					"writeOnly": "true"
				},
				"name": {
					"type": "string"
				},
				"uploadName": {
					"type": "string"
				},
				"width": {
					"type": "integer"
				},
				"height": {
					"type": "integer"
				},
				"dateUploaded": {
					"type": "string",
					"validation": [
						"date_format:Y-m-d H:i:s"
					]
				},
				"altText": {
					"type": "string"
				}
			}
		},
		"licenseTerms": {
			"type": "string",
			"multilingual": true,
			"validation": [
				"nullable"
			]
		},
		"publisherInstitution": {
			"type": "string",
			"validation": [
				"nullable"
			]
		}
	}
}
