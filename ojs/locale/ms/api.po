msgid "api.submissions.403.unpublishedIssues"
msgstr ""
"Anda tidak mempunyai kebenaran untuk melihat terbitan yang belum publikasi."

msgid "api.submissions.403.cantChangeContext"
msgstr "Anda tidak boleh mengubah jurnal yang diserahan."

msgid "api.emails.403.disabled"
msgstr "Ciri pemberitahuan e-mel belum diaktifkan untuk jurnal ini."

msgid "api.announcements.400.contextsNotMatched"
